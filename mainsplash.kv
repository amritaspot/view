<choosemode>:
    BoxLayout:
        orientation: 'vertical'
        canvas.before:
            Color:
                rgba: (1, 1, 1, 1)
            Rectangle:
                size: self.size
                pos: self.pos
        GridLayout:
            cols: 3
            rows: 1
            spacing: 10
            size_hint_y: 0.8
            Button:
                size_hint_y: None
                size_hint_x: None
                height: 100
                width: 100
                background_normal: 'choosemode_test.png'
                on_press: root.manager.current = 'sampleid'
            Button:
                size_hint_y: None
                size_hint_x: None
                height: 100
                width: 100
                background_normal: 'choosemode_calibrate.png'
                on_press: root.manager.current = 'instructionc'
            Button:
                size_hint_y: None
                size_hint_x: None
                height: 100
                width: 100
                background_normal: 'choosemode_view.png'
                on_press: root.manager.current = 'history'
        BoxLayout:
            size_hint_y:0.1
            Button:
                text: 'ShutDown'
                on_press: root.shutdown()

