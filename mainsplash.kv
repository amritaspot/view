<mainsplash>:
    BoxLayout:
        orientation: 'vertical'
        canvas.before:
            Color:
                rgba: (0.06, 0.45, 0.45, 1)
            Rectangle:
                size: self.size
                pos: self.pos
        #figure how to shut down the app
        BoxLayout:
            size_hint_y: 0.95
            Image:
                source: 'logo.png'
        BoxLayout:
            size_hint_y:0.05
            Button:
                text: 'Continue'
                on_press: root.manager.current = 'userid'
            #Label:
            #    text: 'click anywhere to continue'
                #include the logic for click anywhere to continue
