<mainsplash>:
    BoxLayout:
        orientation: 'vertical'
        canvas.before:
            Color:
                rgba: (1, 1, 1, 1)
            Rectangle:
                size: self.size
                pos: self.pos
        BoxLayout:
            size_hint_y: 0.8
            Image:
                source: 'logos.png'
        BoxLayout:
            size_hint_y:0.1
            Button:
                text: 'Continue'
                on_press: root.manager.current = 'userid'
        BoxLayout:
            size_hint_y:0.1
            Button:
                text: 'ShutDown'
                on_press: root.shutdown()

<KeyboardButton@Button>:
    background_color: (0.06, 0.45, 0.45, 1)
    background_normal: ''
    bold: True
    font_size: 10

